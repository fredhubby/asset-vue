abel=\"item.label\"\n                  :value=\"item.value\">\n              </el-option>\n            </el-select>\n          </el-form-item>\n        </el-col>\n\n        <el-col :span=\"9\">\n          <el-form-item label=\"费用:\" class=\"form_item\">\n            <el-input v-model=\"form1.form_fee\" clearable></el-input>\n          </el-form-item>\n        </el-col>\n      </el-row>\n      <el-row :gutter=\"60\">\n        <el-col :span=\"9\" :offset=\"1\">\n          <el-form-item label=\"费用支付日期：\" class=\"form_item\">\n            <el-date-picker\n                v-model=\"form1.form_paidDate\"\n                type=\"date\"\n                placeholder=\"选择日期\"\n                style=\"width: 100%\">\n            </el-date-picker>\n          </el-form-item>\n        </el-col>\n      </el-row>\n\n      <el-divider></el-divider>\n      <span style=\"font-family: 黑体;font-size: 18px\">借据号</span>\n      <el-form-item\n          v-for=\"(iou, index) in form1.form_ious\"\n          :label=\"'借据号' + index\"\n          :key=\"iou.key\"\n          style=\"margin-top: 20px;margin-left: 200px\">\n        <el-input v-model=\"iou.value\" style=\"width: 30%\"></el-input>\n        <el-button\n            @click.prevent=\"removeIou(iou)\"\n            icon=\"el-icon-delete\"\n            circle\n            type=\"danger\"\n            style=\"margin-left: 20px\">\n        </el-button>\n      </el-form-item>\n      <el-form-item style=\"margin-left: 200px\">\n        <el-button type=\"primary\" @click=\"addIou\">新增借据</el-button>\n        <el-button @click=\"resetIou\">重置</el-button>\n      </el-form-item>\n\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport {default as api} from \"@/utils/api\";\n\nexport default {\n  name: \"fee\",\n  data(){\n    return{\n      activeIndex: '3',\n      form1:{\n        form_feeType:'',\n        form_fee:'',\n        form_ious:[{key:'',value:''}],\n        form_paidDate:''\n      },\n      fee_options:[\n        {\n          value:'受让后