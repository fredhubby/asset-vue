efs[formName].validate((valid) => {\n        if (valid) {\n\n          this.$router.push({ name: 'Login' })\n        } else {\n          alert('error register!!')\n          return false\n        }\n      })\n    },\n    back () {\n      this.$router.push('/')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.register-box {\n  width: 400px;\n  margin: 100px auto;\n  border: 1px solid #DCDFE6;\n  padding: 40px;\n  border-radius: 10px;\n  box-shadow: 0 0 50px #DCDFE6;\n}\n\n.register-title {\n  text-align: center;\n}\n</style>\n"]}]}