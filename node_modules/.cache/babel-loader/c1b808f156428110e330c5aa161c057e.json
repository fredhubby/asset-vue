QAAA,WAAA,EAAA,CACA;AACA,UAAA,QAAA,EAAA,IADA;AAEA,UAAA,OAAA,EAAA,SAFA;AAGA,UAAA,OAAA,EAAA;AAHA,SADA,EAMA;AACA,UAAA,GAAA,EAAA,CADA;AAEA,UAAA,GAAA,EAAA,EAFA;AAGA,UAAA,OAAA,EAAA,aAHA;AAIA,UAAA,OAAA,EAAA;AAJA,SANA,EAYA;AACA,UAAA,SAAA,EAAA,gBADA;AAEA,UAAA,OAAA,EAAA;AAFA,SAZA,CA3BA;AA4CA,QAAA,KAAA,EAAA,CACA;AACA,UAAA,QAAA,EAAA,IADA;AAEA,UAAA,OAAA,EAAA,UAFA;AAGA,UAAA,OAAA,EAAA;AAHA,SADA,EAMA;AACA,UAAA,GAAA,EAAA,EADA;AAEA,UAAA,GAAA,EAAA,EAFA;AAGA,UAAA,OAAA,EAAA,WAHA;AAIA,UAAA,OAAA,EAAA;AAJA,SANA;AA5CA;AATA,KAAA;AAoEA,GAlFA;AAmFA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,CADA,EACA;AACA,MAAA,CAAA,CAAA,MAAA,CAAA,KAAA,GAAA,CAAA,CAAA,MAAA,CAAA,KAAA,CAAA,OAAA,CAAA,QAAA,EAAA,EAAA,CAAA;AACA,KAHA;AAIA,IAAA,QAJA,oBAIA,QAJA,EAIA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,KAAA,GAAA,MAAA;AAEA,UAAA,GAAA,CAAA;AACA,YAAA,GAAA,EAAA,oBADA;AAEA,YAAA,MAAA,EAAA,MAFA;AAGA,YAAA,IAAA,EAAA;AACA,cAAA,QAAA,EAAA,KAAA,CAAA,IAAA,CAAA,IADA;AAEA,cAAA,QAAA,EAAA,KAAA,CAAA,IAAA,CAAA,QAFA;AAGA,cAAA,IAAA,EAAA,KAAA,CAAA,IAAA,CAAA,EAHA;AAIA,cAAA,KAAA,EAAA,KAAA,CAAA,IAAA,CAAA;AAJA;AAHA,WAAA,CAAA,CASA,IATA,CASA,UAAA,IAAA,EAAA;AACA,gBAAA,IAAA,CAAA,IAAA,CAAA,KAAA,KAAA,SAAA,EAAA;AACA,cAAA,MAAA,CAAA,QAAA,CAAA;AACA,gBAAA,IAAA,EAAA,OADA;AAEA,gBAAA,OAAA,EAAA;AAFA,eAAA;AAIA,aALA,MAKA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,cAAA,MAAA,CAAA,QAAA,CAAA;AAAA,gBAAA,IAAA,EAAA,SAAA;AAAA,gBAAA,OAAA,EAAA;AAAA,eAAA;;AACA,cAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,gBAAA,IAAA,EAAA;AAAA,eAAA;AACA;AACA,WApBA,EAoBA,KApBA,CAoBA,UAAA,GAAA,EAAA;AACA,YAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAtBA;AAwBA,SA3BA,MA2BA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,SAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,MAFA;AAGA,YAAA,IAAA,EAAA;AAHA,WAAA;;AAIA,iBAAA,KAAA;AACA;AACA,OAnCA;AAoCA,KAzCA;AA0CA,IAAA,IA1CA,kBA0CA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AA5CA;AAnFA,CAAA","sourcesContent":["<template>\n  <div class=\"register\">\n    <el-form ref=\"form\" :rules=\"rules\" :model=\"form\" label-width=\"80px\" class=\"register-box\">\n      <h3 class=\"register-title\">用户注册</h3>\n      <el-form-item label=\"用户名\" prop=\"name\">\n        <el-input v-model.trim=\"form.name\" type=\"text\" clearable></el-input>\n      </el-form-item>\n      <el-form-item label=\"密码\" prop=\"password\">\n        <el-input type=\"password\" v-model.trim=\"form.password\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"确认密码\" prop=\"conpassword\">\n        <el-input type=\"password\" v-model.trim=\"form.conpassword\"></el-input>\n      </el-form-item>\n      <el-form-item label=\"身份：\" prop=\"authentic\">\n        <el-radio v-model=\"form.au\" label=\"1\" border>管理员</el-radio>\n        <el-radio v-model=\"form.au\" label=\"2\" border>普通用户</el-radio>\n      </el-form-item>\n      <el-form-item label=\"手机\" prop=\"phone\">\n      <el-input v-model=\"form.phone\"  @keyup.native=\"UpNumber\"></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button @click=\"back\">返回登录</el-button>\n        <el-button type=\"primary\" @click=\"onRegist('form')\">注册</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nimport {default as api} from \"@/utils/api\";\n\nexport default {\n  name: 'Register',\n  data () {\n    let validatePassword = (rule,value,callback)=>{\n      if(value!==''){\n        if(value!==this.form.password){\n          callback(new Error('两次输入密码不一致'))\n          return false\n        }\n        else {\n          callback()\n        }\n      }\n    }\n    return {\n\n      form: {\n        name: '',\n        password: '',\n        conpassword: '',\n        au: '1',\n        phone:''\n      },\n      rules: {\n        name: [\n          {\n            required: true,\n            message: '请输入用户名',\n            trigger: 'blur'\n          },\n          {\n            min: 1,\n            max: 20,\n            message: '长度在 1 到 20 个字符',\n            trigger: 'blur'\n          }\n        ],\n        password: [\n          {\n            required: true,\n            message: '密码不能为空',\n            trigger: 'blur'\n          },\n          {\n            min: 6,\n            max: 50,\n            message: '长度在 6 个字符以上',\n            trigger: 'blur'\n          }\n        ],\n        conpassword: [\n          {\n            required: true,\n            message: '请输入确认密码',\n            trigger: 'blur'\n          },\n          {\n            min: 6,\n            max: 50,\n            message: '长度在 6 个字符以上',\n            trigger: 'blur'\n          },\n          {\n            validator: validatePassword,\n            trigger: 'blur'\n          }\n        ],\n        phone: [\n          {\n            required: true,\n            message: '手机号码不能为空',\n            trigger: 'blur'\n          },\n          {\n            min: 11,\n            max: 11,\n            message: '请输入正确的手机号',\n            trigger: 'blur'\n          }\n        ]\n      }\n    }\n  },\n  methods: {\n    UpNumber(e) {\n      e.target.value = e.target.value.replace(/[^\\d]/g,\"\");\n    },\n    onRegist (formName) {\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n          var _this = this;\n\n          api({\n            url: \"/user/userRegister\",\n            method: \"post\",\n            data:{\n              username:_this.form.name,\n              password:_this.form.password,\n              perm:_this.form.au,\n              phone:_this.form.phone\n            }\n          }).then(data => {\n            if(data.data.datas==='用户名已存在！'){\n              this.$message({\n                type:\"error\",\n                message:'用户名已存在！'\n              })\n            }else {\n              console.log(data);\n              this.$message({type:\"success\", message:\"注册成功！\"});\n              this.$router.push({ name: 'Login' });\n            }\n          }).catch(err => {\n            console.log(err);\n          })\n\n        } else {\n          this.$message({\n            showClose: true,\n            message: '注册失败',\n            type: 'error'})\n          return false\n        }\n      })\n    },\n    back () {\n      this.$router.push('/Login')\n    }\n  }\n}\n</script>\n\n<style scoped>\n.register-box {\n  width: 400px;\n  margin: 100px auto;\n  border: 1px solid #DCDFE6;\n  padding: 40px;\n  border-radius: 10px;\n  box-shadow: 0 0 50px #DCDFE6;\n}\n\n.register-title {\n  text-align: center;\n}\n</style>\n"],"sourceRoot":"src/views"}]}